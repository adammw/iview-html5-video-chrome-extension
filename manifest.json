{
  "manifest_version": 2,
  "name": "iview html5 video replacer",
  "version": "0.0.4",
  "description": "replace flash with html5 videos",
  "content_scripts": [
    {
      "matches": ["*://*.iview.abc.net.au/*"],
      "css": ["content_style.css"],
      "js": ["vendor/videojs/video.js", "content_script.js"]
    }
  ],
  "permissions": [
    "*://*.iview.abc.net.au/*"
  ],
  "web_accessible_resources": [
    "vendor/*"
  ]
}
